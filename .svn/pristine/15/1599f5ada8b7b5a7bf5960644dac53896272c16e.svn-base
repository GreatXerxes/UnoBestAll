    public void loadBusses() {

        SwingWorker worker17 = new SwingWorker() {

            @Override
            protected Object doInBackground() throws Exception {
                try {
                    ListOfBussesInter server = (ListOfBussesInter) ClientSideServerInstance.getServer("127.0.0.1");
                    List<BusFascade> busFascades = server.getAllBusFascades(); //<--Error
                    //TreeMap<String,Bus> busses = server.getBusses();
                    for (BusFascade b : busFascades) {
                        dlmBusses.addElement(b.getReg());
                    }
                } catch (Exception e) {
                    System.out.println(e);
                }

                return null;
            }

            @Override
            protected void done() {

            }
        };

        worker17.execute();

    }

    public void loadRoutes() {

        SwingWorker worker10 = new SwingWorker() {

            @Override
            protected Object doInBackground() throws Exception {
                try {
                    BusServer server = ClientSideServerInstance.getServer("127.0.0.1");
                    List<RouteFascade> routeFascades = server.getAllRoutes();
                    System.out.println(server.getAllRoutes());
                    for (RouteFascade r : routeFascades) {
                        dlmRoutes.addElement(r.getName());
                    }
                } catch (RemoteException ex) {
                    Logger.getLogger(ManageGUI.class.getName()).log(Level.SEVERE, null, ex);
                }

                return null;
            }

            @Override
            protected void done() {

            }
        };

        worker10.execute();

    }

