    private void RemoveBtnActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_RemoveBtnActionPerformed

        SwingWorker worker11 = new SwingWorker() {

            ListOfRoutesInter server = (ListOfRoutesInter) ClientSideServerInstance.getServer("127.0.0.1");

            @Override
            protected Object doInBackground() throws Exception {
                try {
                    String route = (String) RouteList.getSelectedValue();

                    int dialogButton = JOptionPane.showConfirmDialog(null, "Are you sure you want to remove " + server.getRoute(route) + " from the system", "WARNING", JOptionPane.YES_NO_OPTION);

                    if (dialogButton == JOptionPane.YES_OPTION) {
                        server.removeRoute(route);
                        dlmRoutes.removeElement(route);

                    }

                    // System.out.println(server.getAllFascades().size());
                } catch (RemoteException ex) {
                    Logger.getLogger(ManageGUI.class
                            .getName()).log(Level.SEVERE, null, ex);
                }
                return null;
            }

        };
        worker11.execute();
    }//GEN-LAST:event_RemoveBtnActionPerformed

    private void CreateBtnActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_CreateBtnActionPerformed

        SwingWorker worker18 = new SwingWorker() {

            ListOfRoutesInter server = (ListOfRoutesInter) ClientSideServerInstance.getServer("127.0.0.1");

            @Override
            protected Object doInBackground() throws Exception {
                try {
                    String rid = JOptionPane.showInputDialog("Please enter route number:");
                    String desc = JOptionPane.showInputDialog("Please enter route description:");
                    Route r = new Route(rid, desc);
                    ListOfRoutesInter server = (ListOfRoutesInter) ClientSideServerInstance.getServer("127.0.0.1");

                    server.addRoute(r);
                    dlmRoutes.addElement(r.getName());

                    // TODO add your handling code here:
                } catch (RemoteException ex) {
                    Logger.getLogger(ManageGUI.class.getName()).log(Level.SEVERE, null, ex);
                }
                return null;
            }

        };

        worker18.execute();


    }//GEN-LAST:event_CreateBtnActionPerformed

    private void CreateBtn1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_CreateBtn1ActionPerformed

        
        final ManageGUI thisClass = this;
        SwingWorker worker19 = new SwingWorker() {
            
            ListOfBussesInter server = (ListOfBussesInter) ClientSideServerInstance.getServer("127.0.0.1");

            @Override
            protected Object doInBackground() throws Exception {
                try {

                    String i = (String) dlmBusses.lastElement();

                    String registration = i.substring(4);
                    int y = Integer.parseInt(registration) + 1;
                    Bus aa = new Bus(y);
                    server.addBus(aa);

                    dlmBusses.clear();
            //dlmBusses.addElement(aa.getReg());
                    //dlmBusses.lastElement();
                    //dlmBusses.clear();
                    thisClass.loadBusses();

                } // System.out.println(server.getAllFascades().size());
                catch (RemoteException ex) {
                    Logger.getLogger(ManageGUI.class.getName()).log(Level.SEVERE, null, ex);
                }
                return null;
            }

        };

        worker19.execute();


    }//GEN-LAST:event_CreateBtn1ActionPerformed

